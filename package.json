{
  "name": "cocacola-andina",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve",
    "start-mobile": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0",
    "start:qa": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve -c=qa --port=4202",
    "start:stage": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng serve -c=stage --port=4203",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --optimization --build-optimizer --aot --outputHashing=all",
    "build:qa": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build -c=qa --optimization --build-optimizer --aot --outputHashing=all",
    "build:stage": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build -c=stage --optimization --build-optimizer --aot --outputHashing=all",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot --outputHashing=all",
    "build:stats": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --aot --outputHashing=all --stats-json",
    "test-coverage": "ng test --code-coverage",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.base.json src -s --hideGenerator",
    "bundle-report": "webpack-bundle-analyzer dist/cocacola-andina/stats.json"
  },
